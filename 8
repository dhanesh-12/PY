class A:

def __init__(self, s):

self.s = s

def palindrome(self):

rev = self.s[::-1]

if rev == self.s:

print("The string is a palindrome")

else:

print("The string is not a palindrome")

class B(A):

def __init__(self, s):

super().__init__(s)

def palindrome(self):

temp = self.s

rev = 0

while self.s != 0:

dig = self.s % 10

rev = rev * 10 + dig

self.s = self.s // 10

if temp == rev:

print("The number is a palindrome")

else:

print("The number isn't a palindrome")

string_input = input("Enter the string: ")

integer_input = int(input("Enter the integer: "))

a = A(string_input)

b = B(integer_input)

a.palindrome()

b.palindrome()
